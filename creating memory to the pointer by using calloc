#include<stdio.h>
#include<stdlib.h>
int main()
{
	int *ptr;
	int i;
	int n;
	printf("/nEnter number of elemrnts:");
	scanf("%d",&n);
	ptr=(int*)calloc(n,sizeof(int));
	printf("/nEnter elements into array");
	for(i=0;i<n;i++)
	{
		scanf("%d",(ptr+i));
		
	}
	for(i=0;i<n;i++)
	{
		printf("%d",*(ptr+i));
	}
	free(ptr);
}
