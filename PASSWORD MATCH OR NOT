#include <stdio.h>
#include<string.h>
char str1[10],str2[10];
int main() {
printf("Enter a password:");
fgets(str1,sizeof(str1),stdin);
printf("reenter a password:");
fgets(str2,sizeof(str2),stdin);
if(strcmp(str1,str2)==0)
printf("password matched");
else
printf("password mismatched");


    return 0;
}
